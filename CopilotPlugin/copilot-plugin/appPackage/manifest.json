{
    "$schema": "https://developer.microsoft.com/json-schemas/teams/v1.19/MicrosoftTeams.schema.json",
    "manifestVersion": "1.19",
    "version": "1.0.1",
    "id": "${{TEAMS_APP_ID}}",
    "developer": {
        "name": "An Abstract Level LLC",
        "websiteUrl": "https://effortlessapi.com",
        "privacyUrl": "https://effortlessapi.com/rulebook/privacy",
        "termsOfUseUrl": "https://effortlessapi.com/rulebook/terms"
    },
    "icons": {
        "color": "color.png",
        "outline": "outline.png"
    },
    "name": {
        "short": "SSoTmeAgent${{APP_NAME_SUFFIX}}",
        "full": "Full name for SSoTmeAgent"
    },
    "description": {
        "short": "Short description for SSoTmeAgent",
        "full": "Full description for SSoTmeAgent"
    },
    "accentColor": "#FFFFFF",
    "copilotAgents": {
        "declarativeAgents": [
            {
                "id": "declarativeAgent",
                "file": "declarativeAgent.json"
            }
        ]
    },
    "permissions": [
        "identity",
        "messageTeamMembers"
    ],
    "validDomains": []
}